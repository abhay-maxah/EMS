{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "destination": "http://192.168.1.45:3000/api"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}